{
  "servers": {
    "feishu": {
      "url": "http://localhost:8788/mcp",
      "description": "Your AI models and agents can use our official MCP server to access your 飞书（feishu，lark） data in a simple and secure way."
    },
    "context7": {
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      },
      "description": "Context7 pulls up-to-date, version-specific documentation and code examples directly from the source. Paste accurate, relevant documentation directly into tools like Cursor, Claude, or any LLM."
    },
    "linear": {
      "url": "https://mcp.linear.app/mcp",
      "description": "Your AI models and agents can use our official MCP server to access your Linear data in a simple and secure way."
    },
    "github": {
      "url": "https://api.githubcopilot.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GITHUB_PAT}"
      },
      "description": "GitHub Copilot MCP 服务器"
    }
  },
  "oauth": {
    "callbackPort": "${MCP_CALLBACK_PORT:12334}",
    "host": "localhost",
    "clientName": "Dext"
  }
}